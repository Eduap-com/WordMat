/* ----------[ M a c s y m a ]---------- */
/* ---------- Initialization ---------- */
showtime: all$
prederror: false$
/* ---------- Set Theory ---------- */
x: '[a, b, b, c, c, c]$
y: '[d, c, b]$
z: '[b, e, b]$
/* [x \/ y \/ z, x /\ y /\ z] => [{a, b, c, d, e}, {b}] */
[union(x, y, z), intersect(x, y, z)];
/* x \/ y \/ z - x /\ y /\ z => {a, c, d, e} */
setdifference(%[1], %[2]);
remvalue(x, y, z)$
/* Cartesian product of sets => {(a, c), (a, d), (b, c), (b, d)} */
apply('append, outermap("[", [a, b], [c, d]));
